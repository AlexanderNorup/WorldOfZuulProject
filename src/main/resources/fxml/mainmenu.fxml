<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1197.0" stylesheets="@../cssfiles/mainmenu.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="worldofzuul.PresentationLayer.Controllers.MainMenuController">
   <children>
      <ImageView fx:id="characterImageView" fitHeight="150.0" fitWidth="200.0" layoutX="550.0" layoutY="460.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="CENTER" layoutX="64.0" layoutY="97.0" prefHeight="400.0" prefWidth="600.0" spacing="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="141.0" prefWidth="180.0" spacing="40.0">
               <children>
                  <Button fx:id="playGameButton" mnemonicParsing="false" onAction="#playGame" scaleX="3.0" scaleY="3.0" stylesheets="@../cssfiles/buttonstyle.css" text="Start Game" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="Arial Black" size="12.0" />
                     </font></Button>
                  <Label fx:id="playGameLabel" alignment="CENTER" scaleY="2.0" scaleZ="2.0" textAlignment="CENTER">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="588.0" spacing="400.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="141.0" prefWidth="180.0" spacing="40.0">
                     <children>
                        <Button fx:id="selectCharacterButton" mnemonicParsing="false" onMouseClicked="#selectCharacter" scaleX="3.0" scaleY="3.0" stylesheets="@../cssfiles/buttonstyle.css" text="Select Character" textFill="WHITE">
                           <font>
                              <Font name="Arial Black" size="12.0" />
                           </font></Button>
                        <Label fx:id="selectCharacterLabel" alignment="CENTER" scaleX="2.8" scaleY="2.8" scaleZ="2.8" textAlignment="CENTER" textFill="white">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="141.0" prefWidth="180.0" spacing="40.0">
                     <children>
                        <Button fx:id="quitGameButton" mnemonicParsing="false" onAction="#quitGame" scaleX="3.0" scaleY="3.0" stylesheets="@../cssfiles/buttonstyle.css" text="Quit Game" textFill="WHITE">
                           <font>
                              <Font name="Arial Black" size="12.0" />
                           </font></Button>
                        <Label fx:id="quitGameLabel" alignment="CENTER" scaleY="2.0" scaleZ="2.0" textAlignment="CENTER">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="-40.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="-180.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
